# Example of  ~/.ssh/config  file

Host server1
	HostName example.com
	Port 22
	User misha
	IdentityFile ~/.ssh/id_rsa
	IdentitiesOnly yes

Host pi
	HostName 192.168.0.20
	Port 22
	User pi
	IdentityFile ~/.ssh/pi.key
    IdentitiesOnly yes

Host github
	HostName github.com
	User git
	IdentityFile ~/.ssh/github.key
	IdentitiesOnly yes
